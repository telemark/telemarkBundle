{% block ezobjectrelationlist_field %}
  {% set element_size = params.element_size|default('large') %}
  {% spaceless %}
    {% if not ez_is_field_empty( content, field ) %} 
      {% for contentId in field.value.destinationContentIds %}
        {{ render( controller( "ez_content:viewContent", {'contentId': contentId, 'viewType': 'line', 'params':{'element_size':element_size}} ) ) }}
      {% endfor %}
    {% endif %}
  {% endspaceless %}
{% endblock %}