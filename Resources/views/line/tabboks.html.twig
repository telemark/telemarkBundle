{% set element_size = params.tabboks_size|default('large')%}

{% set constrained =  true %}

{% if params.tabboks_size is defined %}
    {% if params.tabboks_size == 'medium' %}
        {% set constrained =  false %}
    {% endif %}
{% endif %}

{% if constrained == true %}
    <div class="content">
      <div class="constrained-content">
{% endif %}
        <div class="tabs js-tabs">
          <ul class="tabs__content">
            {{ render( controller("tfktelemarkBundle:Item:childrenItems", {'locationId': content.contentInfo.mainLocationId,  'params' : { 'class' : ['tabboks_tab'] , 'viewType': 'line', 'element_size': element_size }} )) }}
          </ul>
        </div>
{% if constrained == true %}
      </div>
    </div>
{% endif %}